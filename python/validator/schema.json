  "prohibition_review": {
    "allow_unknown": true,
    "valid-queue": "DF.valid",
    "invalid-queue": "DF.not-valid",
      "cerberus_rules": {
        "event_version": {
          "type": "string",
          "required": true,
          "allowed": ["1.4"]
        },
        "event_type": {
          "type": "string",
          "required": true,
          "allowed": ["prohibition_review"]
        },
        "prohibition_review": {
          "type": "dict",
          "required": true,
          "schema": {
            "form": {
              "type": "dict",
              "required": true,
              "schema": {
                "identification-information": {
                  "type": "dict",
                  "required": true,
                  "schema": {
                    "applicant-role": {
                      "type": "string",
                      "required": true,
                      "allowed": ["driver", "lawyer", "advocate"]
                    },
                    "first-name-applicant": {
                      "type": "string",
                      "required": true
                    },
                    "last-name-applicant": {
                      "type": "string",
                      "required": true
                    },
                    "applicant-email-address": {
                      "type": "string",
                      "required": true
                    }
                  }
                },
                "prohibition-information": {
                  "type": "dict",
                  "required": true,
                  "schema": {
                    "control-prohibition-number": {
                      "type": "string",
                      "required": true
                    },
                    "prohibition-number-clean": {
                      "type": "string",
                      "required": true
                    },
                    "date-of-service": {
                      "type": "string",
                      "required": true
                    },
                    "licence-seized": {
                      "type": "string",
                      "required": true,
                      "nullable": true
                    },
                    "licence-not-surrendered": {
                      "type": "string",
                      "required": true,
                      "nullable": true
                    },
                    "licence-lost-or-stolen": {
                      "type": "string",
                      "required": true,
                      "nullable": true
                    },
                    "licence-not-issued": {
                      "type": "string",
                      "required": true,
                      "nullable": true
                    }
                  }
                },
                "review-information": {
                  "type": "dict",
                  "required": true,
                  "schema": {
                    "hearing-request-type": {
                      "type": "string",
                      "required": true,
                      "nullable": true
                    }
                  }
                }
              }
            }
          }
        }
      }
  },
