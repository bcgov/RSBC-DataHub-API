{
  "evt_issuance": {
    "allow_unknown": true,
    "valid-queue": "ETK.valid",
    "invalid-queue": "ETK.not-valid",
    "cerberus_rules": {
      "event_id": {
        "type": "integer",
        "required": true
      },
      "event_version": {
        "type": "string",
        "required": true
      },
      "event_date_time": {
        "type": "string",
        "required": true
      },
      "event_type": {
        "type": "string",
        "required": true,
        "allowed": [
          "evt_issuance"
        ]
      },
      "evt_issuance": {
        "type": "dict",
        "required": true,
        "schema": {
          "ticket_number": {
            "type": "string",
            "required": true
          },
          "submit_date": {
            "type": "string",
            "required": true
          },
          "sent_time": {
            "type": "string",
            "required": true
          },
          "violation_date": {
            "type": "string",
            "required": true
          },
          "violation_time": {
            "type": "string",
            "required": true
          },
          "enforcement_jurisdiction_code": {
            "type": "string",
            "required": true
          },
          "certificate_of_service_date": {
            "type": "string",
            "required": true
          },
          "certificate_of_service_number": {
            "type": "string",
            "required": true
          },
          "e_violation_form_number": {
            "type": "string",
            "required": true
          },
          "counts": {
            "type": "list",
            "required": true,
            "schema": {
              "type": "dict",
              "schema": {
                "act_code": {
                  "type": "string",
                  "required": true
                },
                "count_number": {
                  "type": "integer",
                  "required": true
                },
                "section_text": {
                  "type": "string",
                  "required": true
                },
                "section_desc": {
                  "type": "string",
                  "required": true,
                  "empty": false
                },
                "fine_amount": {
                  "type": "string",
                  "required": true
                }
              }
            }
          }
        }
      }
    }
  },
