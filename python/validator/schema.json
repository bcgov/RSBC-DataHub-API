{
  "basic_message_structure": {
    "allow_unknown": true,
    "cerberus_rules": {
      "event_id": {
        "type": "integer",
        "required": true
      },
      "event_version": {
        "type": "string",
        "required": true
      },
      "event_date_time": {
        "type": "string",
        "required": true
      },
      "event_type": {
        "type": "string",
        "required": true,
        "allowed": [
          "evt_issuance",
          "vt_query",
          "vt_payment",
          "vt_dispute",
          "vt_dispute_status_update",
          "vt_dispute_finding"
        ]
      }
    }
  },
